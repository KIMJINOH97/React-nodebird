{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "brower": true,
        "node": true,
        "es6": true
    },
    "extends": ["eslint:recommended", "plugin: react/recommended"],
    "plugins": ["import", "react-hooks"],
    "rules": {
        "no-underscore-dangle": "off"
        // "react/forbid-prop-types": 0
    }
}
